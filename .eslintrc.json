{
    "rules": {
        "curly": 2,
        "eqeqeq": 2,
        "no-caller": 2,
        "no-empty": 0,
        "no-new": 2,
        "no-invalid-regexp": 2,
        "no-control-regex": 2,
        "no-regex-spaces": 2,
        "no-undef": 2,
        "strict": 2,
        "no-unused-vars": [0, {
            "vars": "all",
            "args": "none"
        }],
        "semi": 2,
        "no-new-object": 2,
        "indent": [0, 4],
        "valid-jsdoc": 2,
        "no-trailing-spaces": 2,
        "eol-last": 0,
        "max-len": [1, 120]
    },
    "globals": {
        "require": false,
        "console": false,
        "module": false
    }
}
